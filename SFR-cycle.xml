            </Separations>
        </config>
    </prototype>
    <!-- *********************** -->

    <!--      FBR blanket E2 storage     -->


    <prototype>
        <name>Storage_E1</name>
        <config>
            <Storage>
                <in_commods>
                    <val>E1</val>
                </in_commods>
                <out_commods> <val>E1_str</val></out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>
    <!-- *********************** -->


    <prototype>
        <name>Storage_E2</name>
        <config>
            <Storage>
                <in_commods>
                    <val>E2</val>
                </in_commods>
                <out_commods> <val>E2_str</val></out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>


</My_include>
