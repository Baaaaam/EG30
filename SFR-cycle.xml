<My_include>

    <prototype>
        <name>FBR_driver_fabrication</name>
        <config>
            <Mixer>
                <in_streams>
                    <stream>
                        <info>
                            <mixing_ratio>0.913212</mixing_ratio>
                            <buf_size>4000</buf_size>
                        </info>
                        <commodities>
                            <item>
                                <commodity>drv_tru_str</commodity>
                                <pref>100</pref>
                            </item>
                        </commodities>
                    </stream>
                    <stream>
                        <info>
                            <mixing_ratio>8.67881E-02</mixing_ratio>
                            <buf_size>380</buf_size>
                        </info>
                        <commodities>
                            <item>
                                <commodity>pwr_tru_str</commodity>
                                <pref>100</pref>
                            </item>
                            <item>
                                <commodity>blk_tru_str</commodity>
                                <pref>10</pref>
                            </item>
                        </commodities>
                    </stream>
                </in_streams>

                <throughput>4375</throughput>
                <out_commod>FBR_MOX_N</out_commod>
                <out_buf_size>4375</out_buf_size>

            </Mixer>
        </config>
    </prototype>

    <prototype>
        <name>FBR_driver</name>
        <lifetime>720</lifetime>
        <config>
            <Reactor>
                <fuel_inrecipes>
                    <val>FBR_LEU_N</val>
                    <val>FBR_MOX_N_1</val>
                    <val>FBR_MOX_N_2</val>
                    <val>FBR_MOX_N_3</val>
                    <val>FBR_MOX_N_4</val>
                    <val>FBR_MOX_N</val>
                </fuel_inrecipes>
                <fuel_incommods>
                    <val>FBR_LEU_N</val>
                    <val>FBR_MOX_N_1</val>
                    <val>FBR_MOX_N_2</val>
                    <val>FBR_MOX_N_3</val>
                    <val>FBR_MOX_N_4</val>
                    <val>FBR_MOX_N</val>
                </fuel_incommods>

                <fuel_outrecipes>
                    <val>FBR_LEU_S</val>
                    <val>FBR_MOX_S_1</val>
                    <val>FBR_MOX_S_2</val>
                    <val>FBR_MOX_S_3</val>
                    <val>FBR_MOX_S_4</val>
                    <val>FBR_MOX_S</val>
                </fuel_outrecipes>
                <fuel_outcommods>
                    <val>FBR_LEU_S</val>
                    <val>FBR_MOX_S_1</val>
                    <val>FBR_MOX_S_2</val>
                    <val>FBR_MOX_S_3</val>
                    <val>FBR_MOX_S_4</val>
                    <val>FBR_MOX_S</val>
                </fuel_outcommods>
                <fuel_prefs>
                    <val>1</val>
                    <val>10</val>
                    <val>100</val>
                    <val>1000</val>
                    <val>10000</val>
                    <val>100000</val>
                </fuel_prefs>
                <cycle_time>13</cycle_time>
                <refuel_time>0</refuel_time>
                <assem_size>4264.9</assem_size>
                <n_assem_core>4</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>400</power_cap>

            </Reactor>
        </config>
    </prototype>

    <prototype>
        <name>FBR_in_blanket</name>
        <lifetime>720</lifetime>
        <config>
            <Reactor>
                <fuel_inrecipes>
                    <val>nu</val>
                    <val>nu</val>
                </fuel_inrecipes>

                <fuel_incommods>
                    <val>pwr_u_str</val>
                    <val>nu</val>
                </fuel_incommods>

                <fuel_prefs>
                    <val>100</val>
                    <val>1</val>
                </fuel_prefs>

                <fuel_outrecipes>
                    <val>FBR_BLT_IN_S</val>
                    <val>FBR_BLT_IN_S</val>
                </fuel_outrecipes>
                <fuel_outcommods>
                    <val>FBR_blanket_spent</val>
                    <val>FBR_blanket_spent</val>
                </fuel_outcommods>

                <cycle_time>13</cycle_time>
                <refuel_time>0</refuel_time>
                <assem_size>931.1</assem_size>
                <n_assem_core>4</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>0.1</power_cap>

            </Reactor>
        </config>
    </prototype>

    <prototype>
        <name>FBR_out_blanket</name>
        <lifetime>720</lifetime>
        <config>
            <Reactor>
                <fuel_inrecipes>
                    <val>nu</val>
                    <val>nu</val>
                </fuel_inrecipes>

                <fuel_incommods>
                    <val>pwr_u_str</val>
                    <val>nu</val>
                </fuel_incommods>

                <fuel_prefs>
                    <val>100</val>
                    <val>1</val>
                </fuel_prefs>

                <fuel_outrecipes>
                    <val>FBR_BLT_OUT_S</val>
                    <val>FBR_BLT_OUT_S</val>
                </fuel_outrecipes>
                <fuel_outcommods>
                    <val>FBR_blanket_spent</val>
                    <val>FBR_blanket_spent</val>
                </fuel_outcommods>
                <cycle_time>13</cycle_time>
                <refuel_time>0</refuel_time>
                <assem_size>643.74</assem_size>
                <n_assem_core>8</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>0.1</power_cap>

            </Reactor>
        </config>
    </prototype>


    <prototype>
        <name>FBR_driver_MOX_cooling</name>
        <config>
            <Storage>
                <in_commods>
                    <val>FBR_driver_MOX_spent</val>
                    <val>FBR_driver_LEU_spent</val>
                </in_commods>
                <out_commods>
                    <val>FBR_driver_cooled</val>
                </out_commods>
                <residence_time>81</residence_time>
            </Storage>
        </config>
    </prototype>

    <prototype>
        <name>FBR_driver_separation</name>
        <config>
            <Separations>
                <feedbuf_size>1e100</feedbuf_size>
                <feed_commods>
                    <val>FBR_driver_cooled</val>
                </feed_commods>
                <feed_commod_prefs>
                    <val>1.0</val>
                </feed_commod_prefs>
                <throughput>1e100</throughput>
                <streams>
                    <item>
                        <commod>drv_tru</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>Pu</comp> <eff>9.88e-01</eff> </item>
                                <item> <comp>U</comp>  <eff>9.88e-01</eff> </item>
                                <item> <comp>Np</comp> <eff>9.88e-01</eff> </item>
                                <item> <comp>Am</comp> <eff>9.88e-01</eff> </item>
                                <item> <comp>Cm</comp> <eff>9.88e-01</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                </streams>
                <leftover_commod>waste</leftover_commod>
            </Separations>
        </config>
    </prototype>

    <prototype>
        <name>FBR_blanket_cooling</name>
        <config>
            <Storage>
                <in_commods> <val>FBR_blanket_spent</val></in_commods>
                <out_commods> <val>FBR_blanket_cooled</val></out_commods>
                <residence_time>81</residence_time>
            </Storage>
        </config>
    </prototype>


    <prototype>
        <name>FBR_blanket_separation</name>
        <config>
            <Separations>
                <feedbuf_size>1e100</feedbuf_size>

                <feed_commods>
                    <val>FBR_blanket_cooled</val>
                </feed_commods>

                <feed_commod_prefs>
                    <val>1.0</val>
                </feed_commod_prefs>

                <throughput>1e100</throughput>

                <streams>
                    <item>
                        <commod>blk_tru</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>Pu</comp> <eff>9.88e-01</eff> </item>
                                <item> <comp>U</comp>  <eff>9.88e-01</eff> </item>
                                <item> <comp>Np</comp> <eff>9.88e-01</eff> </item>
                                <item> <comp>Am</comp> <eff>9.88e-01</eff> </item>
                                <item> <comp>Cm</comp> <eff>9.88e-01</eff> </item>
                            </efficiencies>
                        </info>
                    </item>

                </streams>

                <leftover_commod>waste</leftover_commod>

            </Separations>
        </config>
    </prototype>

    <prototype>
        <name>Storage_drv_tru</name>
        <config>
            <Storage>
                <in_commods>
                    <val>drv_tru</val>
                </in_commods>
                <out_commods> <val>drv_tru_str</val></out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

    <prototype>
        <name>Storage_blk_tru</name>
        <config>
            <Storage>
                <in_commods>
                    <val>blk_tru</val>
                </in_commods>
                <out_commods> <val>blk_tru_str</val></out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>


</My_include>
