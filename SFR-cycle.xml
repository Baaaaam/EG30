<My_include>

    <!--      FBR fabrication      -->
    <prototype>
        <name>FBR_fabrication</name>
        <config>
            <Mixer>
                <in_streams>
                    <stream>
                        <info>
                            <mixing_ratio>0.658581</mixing_ratio>
                            <buf_size>3300</buf_size>
                        </info>
                        <commodities>
                            <item>
                                <commodity>E1_str</commodity>
                                <pref>10</pref>
                            </item>
                            <item>
                                <commodity>E1_alt</commodity>
                                <pref>1</pref>
                            </item>
                        </commodities>
                    </stream>

                    <stream>
                        <info>
                            <mixing_ratio>0.0625890</mixing_ratio>
                            <buf_size>350</buf_size>
                        </info>
                        <commodities>
                            <item>
                                <commodity>J1_TRU_str</commodity>
                                <pref>10</pref>
                            </item>
                            <item>
                                <commodity>J1_TRU_alt</commodity>
                                <pref>1</pref>
                            </item>
                        </commodities>
                    </stream>

                    <stream>
                        <info>
                            <mixing_ratio>0.27883</mixing_ratio>
                            <buf_size>1400</buf_size>
                        </info>
                        <commodities>
                            <item>
                                <commodity>J1_U_str</commodity>
                                <pref>10</pref>
                            </item>
                            <item>
                                <commodity>NU</commodity>
                                <pref>1</pref>
                            </item>
                        </commodities>
                    </stream>

                </in_streams>

                <throughput>5000</throughput>
                <out_commod>FBR_MOX_N</out_commod>
                <out_buf_size>5000</out_buf_size>

            </Mixer>
        </config>
    </prototype>

    <prototype>
        <name>FBR</name>
        <config>
            <FleetReactor>
                <fuel_inrecipes>  <val>FBR_MOX_N</val></fuel_inrecipes>
                <fuel_incommods>  <val>FBR_MOX_N</val></fuel_incommods>

                <fuel_outrecipes> <val>FBR_MOX_S</val></fuel_outrecipes>
                <fuel_outcommods> <val>FBR_MOX_S</val></fuel_outcommods>

                <cycle_time>12</cycle_time>
                <batch_size>4953</batch_size>
                <core_size>27120</core_size>
                <power_cap>360</power_cap>

            </FleetReactor>
        </config>
    </prototype>



    <!-- *********************** -->

    <!--       FBR driver Cooling       -->
    <prototype>
        <name>FBR_cooling</name>
        <config>
            <Storage>
                <in_commods> <val>FBR_MOX_S</val></in_commods>
                <out_commods> <val>FBR_MOX_C</val></out_commods>
                <residence_time>84</residence_time>
            </Storage>
        </config>
    </prototype>
    <!-- *********************** -->


    <!--      FBR driver Separation     -->
    <prototype>
        <name>FBR_separation</name>
        <config>
            <Separations>
                <feedbuf_size>107537</feedbuf_size>

                <feed_commods>
                    <val>FBR_MOX_C</val>
                </feed_commods>

                <feed_commod_prefs>
                    <val>1.0</val>
                </feed_commod_prefs>

                <throughput>83333.3333</throughput>

                <streams>
                    <item>
                        <commod>E1</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>U235</comp> <eff>0.668678962</eff> </item> 
                                <item> <comp>U238</comp> <eff>0.668678962</eff> </item>
                                <item> <comp>NP237</comp> <eff>0.908739201</eff> </item>
                                <item> <comp>PU236</comp> <eff>0.951538561</eff> </item>
                                <item> <comp>PU238</comp> <eff>0.981654473</eff> </item>
                                <item> <comp>PU239</comp> <eff>0.808009128</eff> </item>
                                <item> <comp>PU240</comp> <eff>0.962206369</eff> </item>
                                <item> <comp>PU241</comp> <eff>0.976072626</eff> </item>
                                <item> <comp>PU242</comp> <eff>0.987663307</eff> </item>
                                <item> <comp>AM241</comp> <eff>0.984306056</eff> </item>
                                <item> <comp>AM242M</comp> <eff>0.987836518</eff> </item>
                                <item> <comp>AM243</comp> <eff>0.987961612</eff> </item>
                                <item> <comp>CM242</comp> <eff>0.987835149</eff> </item>
                                <item> <comp>CM243</comp> <eff>0.987856243</eff> </item>
                                <item> <comp>CM244</comp> <eff>0.987994753</eff> </item>
                                <item> <comp>CM245</comp> <eff>0.987999096</eff> </item>
                                <item> <comp>CM246</comp> <eff>0.987999984</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                    <item>
                        <commod>E2</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>U235</comp> <eff>0.319321038</eff> </item> 
                                <item> <comp>U238</comp> <eff>0.319321038</eff> </item>
                                <item> <comp>NP237</comp> <eff>0.079260799</eff> </item>
                                <item> <comp>PU236</comp> <eff>0.036461439</eff> </item>
                                <item> <comp>PU238</comp> <eff>0.006345527</eff> </item>
                                <item> <comp>PU239</comp> <eff>0.179990872</eff> </item>
                                <item> <comp>PU240</comp> <eff>0.025793631</eff> </item>
                                <item> <comp>PU241</comp> <eff>0.011927374</eff> </item>
                                <item> <comp>PU242</comp> <eff>0.000336693</eff> </item>
                                <item> <comp>AM241</comp> <eff>0.003693944</eff> </item>
                                <item> <comp>AM242M</comp> <eff>0.000163482</eff> </item>
                                <item> <comp>AM243</comp> <eff>3.83882E-05</eff> </item>
                                <item> <comp>CM242</comp> <eff>0.000164851</eff> </item>
                                <item> <comp>CM243</comp> <eff>0.000143757</eff> </item>
                                <item> <comp>CM244</comp> <eff>5.24746E-06</eff> </item>
                                <item> <comp>CM245</comp> <eff>9.04147E-07</eff> </item>
