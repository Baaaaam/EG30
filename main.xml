<simulation xmlns:xi="http://www.w3.org/2001/XInclude">

  <schematype>flat</schematype>
  <control>
    <duration>1200</duration>
    <startmonth>1</startmonth>
    <startyear>2015</startyear>
    <decay>never</decay>
