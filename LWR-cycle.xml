<My_include>
    <prototype>
        <name>PWR_fabrication</name>
        <config>
            <cyclassFuelFab>

                <eq_model>MLP_Kinf</eq_model>
                <eq_command>/Users/mouginot/work/MODEL/PWR/EQM/weights/REP_MOX_TRU_34.2262Wg.xml,/Users/mouginot/work/MODEL/PWR/EQM/weights/REP_MOX_TRU_34.2262Wg.nfo,4,1.034</eq_command>

                <fiss_commods>
                    <val>blk_tru_str</val>
                </fiss_commods>
                <fiss_commod_prefs>
                    <val>1.0</val>
                </fiss_commod_prefs>
                <fiss_size>89000</fiss_size>


                <fill_commods>
                    <val>nu</val>
                </fill_commods>
                <fill_recipe>nu</fill_recipe>
                <fill_commod_prefs>
                    <val>1.0</val>
                </fill_commod_prefs>
                <fill_size>89000</fill_size>

                <throughput>89000</throughput>
                <outcommod>PWR_MOX_new</outcommod>
            </cyclassFuelFab>
        </config>
    </prototype>


    <prototype>
        <name>PWR</name>
        <lifetime>720</lifetime>
        <config>
            <cyclassReactor>
                <eq_model>MLP_Kinf</eq_model>
                <eq_command>/Users/mouginot/work/MODEL/PWR/EQM/weights/REP_MOX_TRU_34.2262Wg.xml,/Users/mouginot/work/MODEL/PWR/EQM/weights/REP_MOX_TRU_34.2262Wg.nfo,4,1.034</eq_command>

                <ir_model>RK4</ir_model>
                <ir_command>none</ir_command>

                <xs_model>XSM_MLP</xs_model>
                <xs_command>/Users/mouginot/work/MODEL/PWR/XS/Generate/weights/,Data_Base_Info.nfo,false</xs_command>

                <fuel_incommods>
                    <val>PWR_MOX_new</val>
                </fuel_incommods>
                <fuel_prefs>
                    <val>10</val>
                </fuel_prefs>

                <fuel_outcommods>
                    <val>PWR_MOX_spent</val>
                </fuel_outcommods>
                <cycle_time>50</cycle_time> <!-- BU/30days *Mcore/batch / P_eff = 50./(30)*88758./3./(3000*0.9) -->
                <refuel_time>0</refuel_time>
                <assem_size>88758</assem_size><!-- Core_inv / Core_Ass = 88758 / 3. -->
                <n_assem_core>1</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>1000</power_cap> <!-- P_th * Ef_th * Cap_fac = 3000*33.333%*0.9 -->
                <power>3000</power>
                <burnup>50</burnup>

            </cyclassReactor>
        </config>
    </prototype>



    <prototype>
        <name>PWR_cooling</name>
        <config>
            <Storage>
                <in_commods> <val>PWR_MOX_spent</val></in_commods>
                <out_commods> <val>PWR_MOX_spent_cooled</val></out_commods>
                <residence_time>81</residence_time>
            </Storage>
        </config>
    </prototype>


    <prototype>
        <name>PWR_separation</name>
        <config>
            <Separations>
                <feedbuf_size>1e100</feedbuf_size>
                <feed_commods>
                    <val>PWR_MOX_spent_cooled</val>
                </feed_commods>

                <feed_commod_prefs>
                    <val>1.0</val>
                </feed_commod_prefs>

                <throughput>1e100</throughput>

                <streams>
                    <item>
                        <commod>pwr_tru</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>Pu</comp> <eff>9.88E-01</eff> </item>
                                <item> <comp>234U</comp>  <eff>3.952E-01</eff> </item>
                                <item> <comp>235U</comp>  <eff>3.952E-01</eff> </item>
                                <item> <comp>236U</comp>  <eff>3.952E-01</eff> </item>
                                <item> <comp>238U</comp>  <eff>3.952E-01</eff> </item>
                                <item> <comp>Np</comp> <eff>9.88E-01</eff> </item>
                                <item> <comp>Am</comp> <eff>9.88E-01</eff> </item>
                                <item> <comp>Cm</comp> <eff>9.88E-01</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                    <item>
                        <commod>ru</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>235U</comp>  <eff>3.952E-01</eff> </item>
                                <item> <comp>238U</comp>  <eff>3.952E-01</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                </streams>
                <leftover_commod>waste</leftover_commod>
            </Separations>
        </config>
    </prototype>



    <prototype>
        <name>Storage_pwr_tru</name>
        <config>
            <Storage>
                <in_commods>
                    <val>pwr_tru</val>
                </in_commods>
                <out_commods>
                    <val>pwr_tru_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>



    <prototype>
        <name>Storage_pwr_u</name>
        <config>
            <Storage>
                <in_commods>
                    <val>pwr_u</val>
                </in_commods>
                <out_commods>
                    <val>pwr_u_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

</My_include>


