<My_include>
    <prototype>
        <name>PWR_fabrication</name>
        <config>
            <Mixer>
                <in_streams>
                    <stream>
                        <info>
                            <mixing_ratio>0.879677</mixing_ratio> <!-- throughput * mixing_ratio = 29586.0 * 0.879677 -->
                            <buf_size>6600</buf_size>
                        </info>
                        <commodities>
                            <item>
                                <commodity>blk_tru_str</commodity>
                                <pref>100</pref>
                            </item>
                        </commodities>
                    </stream>
                    <stream>
                        <info>
                            <mixing_ratio>0.120323</mixing_ratio> <!-- throughput * mixing_ratio = 29586.0 * 0.120323 -->
                            <buf_size>910</buf_size>
                        </info>
                        <commodities>
                            <item>
                                <commodity>nu</commodity>
                                <pref>1</pref>
                            </item>
                        </commodities>
                    </stream>
                </in_streams>
                <out_commod>PWR_MOX_new</out_commod>
                <throughput>7500</throughput> <!-- 1 Core / month -->
                <out_buf_size>30000</out_buf_size>
            </Mixer>
        </config>
    </prototype>



    <prototype>
        <name>PWR</name>
        <lifetime>720</lifetime>
        <config>
            <Reactor>
                <fuel_inrecipes>  <val>PWR_MOX_N</val></fuel_inrecipes>
                <fuel_incommods>  <val>PWR_MOX_new</val></fuel_incommods>

                <fuel_outrecipes> <val>PWR_MOX_S</val></fuel_outrecipes>
                <fuel_outcommods> <val>PWR_MOX_spent</val></fuel_outcommods>
                <cycle_time>18</cycle_time> <!-- BU/30days *Mcore/batch / P_eff = 50./(30)*88758./3./(3000*0.9) -->
                <refuel_time>0</refuel_time>
                <assem_size>29586.0</assem_size><!-- Core_inv / Core_Ass = 88758 / 3. -->
                <n_assem_core>3</n_assem_core>
                <n_assem_batch>1</n_assem_batch>
                <power_cap>1000</power_cap> <!-- P_th * Ef_th * Cap_fac = 3000*33.333%*0.9 -->

            </Reactor>
        </config>
    </prototype>



    <prototype>
        <name>PWR_cooling</name>
        <config>
            <Storage>
                <in_commods> <val>PWR_MOX_spent</val></in_commods>
                <out_commods> <val>PWR_MOX_spent_cooled</val></out_commods>
                <residence_time>81</residence_time>
            </Storage>
        </config>
    </prototype>


    <prototype>
        <name>PWR_separation</name>
        <config>
            <Separations>
                <feedbuf_size>1e100</feedbuf_size>
                <feed_commods>
                    <val>PWR_MOX_spent_cooled</val>
                </feed_commods>

                <feed_commod_prefs>
                    <val>1.0</val>
                </feed_commod_prefs>

                <throughput>1e100</throughput>

                <streams>
                    <item>
                        <commod>pwr_tru</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>Pu</comp> <eff>9.88E-01</eff> </item>
                                <item> <comp>U</comp>  <eff>1.99320E-01</eff> </item>
                                <item> <comp>Np</comp> <eff>9.88E-01</eff> </item>
                                <item> <comp>Am</comp> <eff>9.88E-01</eff> </item>
                                <item> <comp>Cm</comp> <eff>9.88E-01</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                    <item>
                        <commod>pwr_u</commod>
                        <info>
                            <buf_size>1e100</buf_size>
                            <efficiencies>
                                <item> <comp>U</comp>  <eff>7.88680E-01</eff> </item>
                            </efficiencies>
                        </info>
                    </item>
                </streams>
                <leftover_commod>waste</leftover_commod>
            </Separations>
        </config>
    </prototype>



    <prototype>
        <name>Storage_pwr_tru</name>
        <config>
            <Storage>
                <in_commods>
                    <val>pwr_tru</val>
                </in_commods>
                <out_commods>
                    <val>pwr_tru_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>



    <prototype>
        <name>Storage_pwr_u</name>
        <config>
            <Storage>
                <in_commods>
                    <val>pwr_u</val>
                </in_commods>
                <out_commods>
                    <val>pwr_u_str</val>
                </out_commods>
                <residence_time>0</residence_time>
            </Storage>
        </config>
    </prototype>

</My_include>


