<My_include>


  <!--      PWR fabrication      -->

  <prototype>
    <name>PWR_fabrication</name>
    <!--    <lifetime>960</lifetime> -->
    <config>
      <Mixer>
        <in_streams>
          <stream>
            <info>
              <mixing_ratio>0.879677</mixing_ratio>
<!-- throughput * mixing_ratio = 29586.0 * 0.879677 -->
              <buf_size>26027</buf_size>
            </info>
            <commodities>
              <item>
                <commodity>E2_str</commodity>
                <pref>10</pref>
              </item>
              <item>
                <commodity>E2_alt</commodity>
                <pref>1</pref>
              </item>
            </commodities>
          </stream>

          <stream>
            <info>
              <mixing_ratio>0.120323</mixing_ratio>
<!-- throughput * mixing_ratio = 29586.0 * 0.120323 -->
              <buf_size>3560</buf_size>
            </info>
            <commodities>
              <item>
                <commodity>NU</commodity>
                <pref>1</pref>
              </item>
            </commodities>
          </stream>

        </in_streams>
        <out_commod>PWR_MOX_new</out_commod>

<!-- 1 Core / month -->
        <throughput>29586.0</throughput>
        <out_buf_size>29586.0</out_buf_size>
<!-- ************** -->

      </Mixer>
    </config>
  </prototype>

  <!-- *********************** -->

  <!--       PWR reactor       -->
  <prototype>
    <name>PWR</name>
    <config>
      <FleetReactor>
        <fuel_inrecipes>  <val>PWR_MOX_N</val></fuel_inrecipes>
        <fuel_incommods>  <val>PWR_MOX_new</val></fuel_incommods>

        <fuel_outrecipes> <val>PWR_MOX_S</val></fuel_outrecipes>
        <fuel_outcommods> <val>PWR_MOX_spent</val></fuel_outcommods>
        <cycle_time>18</cycle_time>
        <batch_size>29586</batch_size>
        <core_size>88758</core_size>
        <power_cap>900</power_cap> 

      </FleetReactor>
    </config>
  </prototype>
<!-- *********************** -->

<!--       PWR Cooling       -->
<prototype>
  <name>PWR_cooling</name>
  <config>
    <Storage>
      <in_commods> <val>PWR_MOX_spent</val></in_commods>
      <out_commods> <val>PWR_MOX_spent_cooled</val></out_commods>
      <residence_time>84</residence_time>
    </Storage>
  </config>
</prototype>
<!-- *********************** -->

<!--       PWR Storage       -->
<prototype>
  <name>PWR_storage</name>
  <config>
    <Storage>
      <in_commods> <val>PWR_MOX_spent_cooled</val></in_commods>
      <out_commods> <val>PWR_MOX_spent_stored</val></out_commods>
      <residence_time>0</residence_time>
    </Storage>
  </config>
</prototype>
<!-- *********************** -->

<!--      PWR Separation     -->
<prototype>
  <name>PWR_separation</name>
  <config>
    <Separations>
      <feedbuf_size>1e100</feedbuf_size>

      <feed_commods>
        <val>PWR_MOX_spent_stored</val>
      </feed_commods>

      <feed_commod_prefs>
        <val>1.0</val>
      </feed_commod_prefs>

      <throughput>1e100</throughput>

      <streams>
        <item>
          <commod>J1_TRU</commod>
          <info>
            <buf_size>1e100</buf_size>
            <efficiencies>
              <item> <comp>Pu</comp> <eff>8.83838E-01</eff> </item>
              <item> <comp>U</comp>  <eff>1.78297E-01</eff> </item>
              <item> <comp>Np</comp> <eff>8.83838E-01</eff> </item>
              <item> <comp>Am</comp> <eff>8.83838E-01</eff> </item>
              <item> <comp>Cm</comp> <eff>8.83838E-01</eff> </item>
            </efficiencies>
          </info>
        </item>
        <item>
          <commod>J1_U</commod>
          <info>
            <buf_size>1e100</buf_size>
            <efficiencies>
              <item> <comp>U</comp>  <eff>7.05541E-01</eff> </item>
            </efficiencies>
          </info>
        </item>
      </streams>
      <leftover_commod>waste</leftover_commod>

    </Separations>
  </config>
</prototype>
<!-- *********************** -->

<!--      PWR U/pu storage     -->

<prototype>
  <name>Storage_J1_TRU</name>
  <config>
    <Storage>
      <in_commods>
        <val>J1_TRU</val>
      </in_commods>
      <out_commods>
        <val>J1_TRU_str</val>
      </out_commods>
      <residence_time>0</residence_time>
    </Storage>
  </config>
</prototype>

<prototype>
  <name>Storage_J1_U</name>
  <config>
    <Storage>
      <in_commods>
        <val>J1_U</val>
      </in_commods>
      <out_commods>
        <val>J1_U_str</val>
      </out_commods>
      <residence_time>0</residence_time>
    </Storage>
  </config>
</prototype>


<!-- *********************** -->

</My_include>


